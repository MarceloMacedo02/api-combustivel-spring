create table abastecimento (id bigint generated by default as identity, data_realizacao date not null, observacao varchar(255), valor decimal(19,2) not null, credor_id bigint not null, veiculo_id bigint not null, combustivel_id bigint not null, setor_id bigint not null, primary key (id))
create table combustivel (id bigint generated by default as identity, descricao varchar(65), primary key (id))
create table combustivel_tipos_combustiveis (combustivel_id bigint not null, tipo_combustivel_id bigint not null)
create table fabricante (id bigint generated by default as identity, fipe_name varchar(255), name varchar(255), primary key (id))
create table item_manutencao (id bigint generated by default as identity, quantidade decimal(19,2), valor decimal(19,2), manutencao_id bigint not null, suprimento_id bigint not null, primary key (id))
create table manutencao (id bigint generated by default as identity, data_realizacao date not null, observacao varchar(255), credor_id bigint not null, veiculo_id bigint not null, primary key (id))
create table modelo (id bigint not null, name varchar(255), fabricante_id bigint, primary key (id))
create table setor (id bigint generated by default as identity, nome varchar(255), primary key (id))
create table suprimento (id bigint generated by default as identity, descricao varchar(150), primary key (id))
create table tipo_combustivel (id bigint generated by default as identity, descricao varchar(150), primary key (id))
create table usuario (id bigint generated by default as identity, telefone varchar(15), documento varchar(25), bairro varchar(80), cep varchar(10), numero varchar(10), rua varchar(80), nome varchar(80), email varchar(255), perfil integer not null, senha varchar(255), primary key (id))
create table veiculo (id bigint generated by default as identity, alugado integer, ano varchar(255), aquisicao date, ativo integer, chassi varchar(255), chip varchar(255), conservacao varchar(255) not null, consumo_medio float, cor varchar(255), descricao varchar(255), device_id bigint, imei varchar(255), maquinario integer, observacao varchar(255), placa varchar(10), renavan varchar(255), rota varchar(255), tanque float not null, tombamento varchar(255), codigo_fipe varchar(255), codigoano varchar(255), codigomarca varchar(255), codigomodelo varchar(255), combustivel varchar(255), marca varchar(255), mes_referencia varchar(255), modelo varchar(255), sigla_combustivel varchar(255), tipo_veiculo varchar(255), valor varchar(255), modelo_id bigint, responsavel_id bigint not null, setor_id bigint not null, tipo_combustivel_id bigint, primary key (id))
